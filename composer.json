{
  "name": "vovan-ve/html-template",
  "description": "Context sensitive HTML template engine",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vovan-VE",
      "email": "vovan-ve@yandex.ru"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit --verbose"
  },
  "archive": {
    "exclude": [
      "/tests/",
      "/phpunit.xml.dist"
    ]
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "vovan-ve/lr0-parser": "~1.6.0"
  },
  "require-dev": {
    "ext-json": "*",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "VovanVE\\HtmlTemplate\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VovanVE\\HtmlTemplate\\tests\\": "tests"
    }
  }
}
